{"ast":null,"code":"import { Icon } from \"@iconify/vue\";\nimport SideBar from \"@/components/SideBar.vue\";\nimport $store from \"@/store/index\";\nimport axios from \"axios\";\nexport default {\n  name: \"DashBoard\",\n  data() {\n    return {\n      Companies: []\n    };\n  },\n  components: {\n    SideBar,\n    Icon\n  },\n  methods: {\n    changeid(cmid) {\n      $store.state.id = cmid;\n    },\n    getdata() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-companies\",\n        method: \"get\",\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(respnose => {\n        this.Companies = respnose.data;\n      }).catch(error => {\n        window.alert(error.respnose);\n      });\n    }\n  },\n  beforeMount() {\n    this.getdata();\n  }\n};","map":{"version":3,"names":["Icon","SideBar","$store","axios","name","data","Companies","components","methods","changeid","cmid","state","id","getdata","token","window","localStorage","getItem","url","method","headers","Authorization","then","respnose","catch","error","alert","beforeMount"],"sources":["E:\\Work\\Projects\\work_cycle\\src\\views\\DashBoard.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard\">\n    <SideBar>\n      <div class=\"container\">\n        <div class=\"grey-bg container-fluid\">\n          <section id=\"minimal-statistics\">\n            <div class=\"row\">\n              <div\n                class=\"col-xl-3 col-sm-6 col-12 p-card\"\n                v-for=\"company in Companies\"\n                :key=\"company.name\"\n              >\n                <RouterLink to=\"/companyinfo\" @click=\"changeid(company.id)\">\n                  <div class=\"card my_card\">\n                    <div class=\"card-content\">\n                      <div class=\"card-body\">\n                        <div class=\"media d-flex\">\n                          <div class=\"align-self-center valign-self-right\">\n                            <Icon\n                              icon=\"mdi:company\"\n                              color=\"royalblue\"\n                              width=\"40\"\n                              height=\"40\"\n                            />\n                          </div>\n                          <div\n                            class=\"media-body\"\n                            style=\"text-align: right; width: 100%\"\n                          >\n                            <h3>{{ company.name }}</h3>\n                            <span>Users:{{ company.num_of_users }}</span>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </RouterLink>\n              </div>\n            </div>\n          </section>\n        </div>\n      </div>\n    </SideBar>\n  </div>\n  <!-- background -->\n  <img src=\"@/assets/Images/Background/Background3.jpg\" class=\"bgimg\" alt=\"\" />\n</template>\n\n<script>\nimport { Icon } from \"@iconify/vue\";\nimport SideBar from \"@/components/SideBar.vue\";\nimport $store from \"@/store/index\";\nimport axios from \"axios\";\nexport default {\n  name: \"DashBoard\",\n  data() {\n    return {\n      Companies: [],\n    };\n  },\n  components: {\n    SideBar,\n    Icon,\n  },\n  methods: {\n    changeid(cmid) {\n      $store.state.id = cmid;\n    },\n    getdata() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-companies\",\n        method: \"get\",\n        headers: { Authorization: `Bearer ${token}` },\n      })\n        .then((respnose) => {\n          this.Companies = respnose.data;\n        })\n        .catch((error) => {\n          window.alert(error.respnose);\n        });\n    },\n  },\n  beforeMount() {\n    this.getdata();\n  },\n};\n</script>\n\n<style scoped>\nh3 {\n