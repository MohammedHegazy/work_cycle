{"ast":null,"code":"import { Icon } from \"@iconify/vue\";\nimport SideBar from \"@/components/SideBar.vue\";\nimport $store from \"@/store/index\";\nimport axios from \"axios\";\nexport default {\n  name: \"DashBoard\",\n  data() {\n    return {\n      Companies: []\n    };\n  },\n  components: {\n    SideBar,\n    Icon\n  },\n  methods: {\n    changeid(cmid) {\n      $store.state.id = cmid;\n    },\n    sidebar() {\n      window.$(document).ready(function () {\n        var trigger = window.$(\".hamburger\"),\n          overlay = window.$(\".overlay\"),\n          isClosed = false;\n        trigger.click(function () {\n          hamburger_cross();\n        });\n        function hamburger_cross() {\n          if (isClosed == true) {\n            overlay.hide();\n            trigger.removeClass(\"is-open\");\n            trigger.addClass(\"is-closed\");\n            isClosed = false;\n          } else {\n            overlay.show();\n            trigger.removeClass(\"is-closed\");\n            trigger.addClass(\"is-open\");\n            isClosed = true;\n          }\n        }\n        window.$('[data-toggle=\"offcanvas\"]').click(function () {\n          window.$(\"#wrapper\").toggleClass(\"toggled\");\n        });\n      });\n    },\n    getdata() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-companies\",\n        method: \"get\",\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(respnose => {\n        console.log(respnose.data);\n        this.Companies = respnose.data;\n      }).catch(error => {\n        console.log(error);\n      });\n    }\n  },\n  beforeMount() {\n    this.getdata();\n    this.sidebar();\n  }\n};","map":{"version":3,"names":["Icon","SideBar","$store","axios","name","data","Companies","components","methods","changeid","cmid","state","id","sidebar","window","$","document","ready","trigger","overlay","isClosed","click","hamburger_cross","hide","removeClass","addClass","show","toggleClass","getdata","token","localStorage","getItem","url","method","headers","Authorization","then","respnose","console","log","catch","error","beforeMount"],"sources":["E:\\Work\\Projects\\work_cycle\\src\\views\\DashBoard.vue"],"sourcesContent":["<template>\n  <div class=\"dashboard\">\n    <SideBar>\n      <div class=\"container\">\n        <div class=\"grey-bg container-fluid\">\n          <section id=\"minimal-statistics\">\n            <div class=\"row\">\n              <div\n                class=\"col-xl-3 col-sm-6 col-12 p-card\"\n                v-for=\"company in Companies\"\n                :key=\"company.name\"\n              >\n                <RouterLink to=\"/companyinfo\" @click=\"changeid(company.id)\">\n                  <div class=\"card my_card\">\n                    <div class=\"card-content\">\n                      <div class=\"card-body\">\n                        <div class=\"media d-flex\">\n                          <div class=\"align-self-center valign-self-right\">\n                            <Icon\n                              icon=\"mdi:company\"\n                              color=\"royalblue\"\n                              width=\"40\"\n                              height=\"40\"\n                            />\n                          </div>\n                          <div\n                            class=\"media-body\"\n                            style=\"text-align: right; width: 100%\"\n                          >\n                            <h3>{{ company.name }}</h3>\n                            <span>{{ company.num_of_users }}</span>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </div>\n                </RouterLink>\n              </div>\n            </div>\n          </section>\n        </div>\n      </div>\n    </SideBar>\n  </div>\n  <!-- background -->\n  <img src=\"@/assets/Images/Background/Background3.jpg\" class=\"bgimg\" alt=\"\" />\n</template>\n\n<script>\nimport { Icon } from \"@iconify/vue\";\nimport SideBar from \"@/components/SideBar.vue\";\nimport $store from \"@/store/index\";\nimport axios from \"axios\";\nexport default {\n  name: \"DashBoard\",\n  data() {\n    return {\n      Companies: [],\n    };\n  },\n  components: {\n    SideBar,\n    Icon,\n  },\n  methods: {\n    changeid(cmid) {\n      $store.state.id = cmid;\n    },\n    sidebar() {\n      window.$(document).ready(function () {\n        var trigger = window.$(\".hamburger\"),\n          overlay = window.$(\".overlay\"),\n          isClosed = false;\n\n        trigger.click(function () {\n          hamburger_cross();\n        });\n\n        function hamburger_cross() {\n          if (isClosed == true) {\n            overlay.hide();\n            trigger.removeClass(\"is-open\");\n            trigger.addClass(\"is-closed\");\n            isClosed = false;\n          } else {\n            overlay.show();\n            trigger.removeClass(\"is-closed\");\n            trigger.addClass(\"is-open\");\n            isClosed = true;\n          }\n        }\n\n        window.$('[data-toggle=\"offcanvas\"]').click(function () {\n          window.$(\"#wrapper\").toggleClass(\"toggled\");\n        });\n      });\n    },\n    getdata() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-companies\",\n        method: \"get\",\n        headers: { Authorization: `Bearer ${token}` },\n      })\n        .then((respnose) => {\n          console.log(respnose.data);\n          this.Companies = respnose.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  beforeMount() {\n    this.getdata();\n    this.sidebar();\n  },\n};\n</script>\n\n<style scoped>\nh3 {\n  margin: 0;\n}\n\ni {\n  font-size: 40px;\n}\n\n.p-card {\n  padding-bottom: 20px;\n}\n\n.my_card {\n  background-color: rgb(220, 220, 220);\n}\n\na:hover,\na:visited,\na:link,\na:active {\n  text-decoration: none;\n  color: #858796;\n}\n\na,\na:visited,\na:link,\na:active {\n  transition: ease-out 0.5s;\n}\n\na:hover {\n  transition: ease-in 0.5s;\n  color: #000066;\n}\n</style>\n"],"mappings":"AAiDA,SAASA,IAAG,QAAS,cAAc;AACnC,OAAOC,OAAM,MAAO,0BAA0B;AAC9C,OAAOC,MAAK,MAAO,eAAe;AAClC,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVN,OAAO;IACPD;EACF,CAAC;EACDQ,OAAO,EAAE;IACPC,QAAQA,CAACC,IAAI,EAAE;MACbR,MAAM,CAACS,KAAK,CAACC,EAAC,GAAIF,IAAI;IACxB,CAAC;IACDG,OAAOA,CAAA,EAAG;MACRC,MAAM,CAACC,CAAC,CAACC,QAAQ,CAAC,CAACC,KAAK,CAAC,YAAY;QACnC,IAAIC,OAAM,GAAIJ,MAAM,CAACC,CAAC,CAAC,YAAY,CAAC;UAClCI,OAAM,GAAIL,MAAM,CAACC,CAAC,CAAC,UAAU,CAAC;UAC9BK,QAAO,GAAI,KAAK;QAElBF,OAAO,CAACG,KAAK,CAAC,YAAY;UACxBC,eAAe,CAAC,CAAC;QACnB,CAAC,CAAC;QAEF,SAASA,eAAeA,CAAA,EAAG;UACzB,IAAIF,QAAO,IAAK,IAAI,EAAE;YACpBD,OAAO,CAACI,IAAI,CAAC,CAAC;YACdL,OAAO,CAACM,WAAW,CAAC,SAAS,CAAC;YAC9BN,OAAO,CAACO,QAAQ,CAAC,WAAW,CAAC;YAC7BL,QAAO,GAAI,KAAK;UAClB,OAAO;YACLD,OAAO,CAACO,IAAI,CAAC,CAAC;YACdR,OAAO,CAACM,WAAW,CAAC,WAAW,CAAC;YAChCN,OAAO,CAACO,QAAQ,CAAC,SAAS,CAAC;YAC3BL,QAAO,GAAI,IAAI;UACjB;QACF;QAEAN,MAAM,CAACC,CAAC,CAAC,2BAA2B,CAAC,CAACM,KAAK,CAAC,YAAY;UACtDP,MAAM,CAACC,CAAC,CAAC,UAAU,CAAC,CAACY,WAAW,CAAC,SAAS,CAAC;QAC7C,CAAC,CAAC;MACJ,CAAC,CAAC;IACJ,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,MAAMC,KAAI,GAAIf,MAAM,CAACgB,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAClD5B,KAAK,CAAC;QACJ6B,GAAG,EAAE,gEAAgE;QACrEC,MAAM,EAAE,KAAK;QACbC,OAAO,EAAE;UAAEC,aAAa,EAAG,UAASN,KAAM;QAAE;MAC9C,CAAC,EACEO,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAChC,IAAI,CAAC;QAC1B,IAAI,CAACC,SAAQ,GAAI+B,QAAQ,CAAChC,IAAI;MAChC,CAAC,EACAmC,KAAK,CAAEC,KAAK,IAAK;QAChBH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;MACpB,CAAC,CAAC;IACN;EACF,CAAC;EACDC,WAAWA,CAAA,EAAG;IACZ,IAAI,CAACd,OAAO,CAAC,CAAC;IACd,IAAI,CAACf,OAAO,C