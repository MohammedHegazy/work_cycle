v class=\"login_form form-group\">\n                  <input\n                    type=\"text\"\n                    v-model=\"comm_num\"\n                    placeholder=\"Enter Commercial Record\"\n                    class=\"form-control login_inp\"\n                    required\n                  />\n                </div>\n                <div class=\"login_form form-group\">\n                  <input\n                    type=\"text\"\n                    v-model=\"idnum\"\n                    placeholder=\"Enter ID Number\"\n                    class=\"form-control login_inp\"\n                    required\n                  />\n                </div>\n                <div class=\"login_form form-group\">\n                  <input\n                    type=\"email\"\n                    v-model=\"email\"\n                    placeholder=\"Enter E-mail\"\n                    class=\"form-control login_inp\"\n                    required\n                  />\n                </div>\n                <div class=\"login_form form-group d-flex align-items-center\">\n                  <input\n                    type=\"password\"\n                    v-model=\"password\"\n                    placeholder=\"Enter your password\"\n                    class=\"form-control login_inp\"\n                    id=\"id_password\"\n                    required\n                  />\n                  <Icon\n                    icon=\"solar:eye-broken\"\n                    color=\"rgba(150, 150, 150, 0.4666666666666667)\"\n                    id=\"togglePassword\"\n                    style=\"margin-left: -30px; z-index: 2; cursor: pointer\"\n                    @click=\"showpass(1)\"\n                  />\n                </div>\n                <div class=\"login_form form-group d-flex align-items-center\">\n                  <input\n                    type=\"password\"\n                    v-model=\"confpassword\"\n                    placeholder=\"Confirm your password\"\