{"ast":null,"code":"import SideBar from \"@/components/SideBar.vue\";\nimport axios from \"axios\";\nlet test = [];\nexport default {\n  name: \"OrdersView\",\n  data() {\n    return {\n      companies: test\n    };\n  },\n  components: {\n    SideBar\n  },\n  methods: {\n    getorders() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        method: \"get\",\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-all-orders\",\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(function (response) {\n        console.log(response.data);\n        test = response.data;\n      }).catch(function (error) {\n        console.log(error.response);\n      });\n    }\n  },\n  created() {\n    this.getorders();\n    console.log(test);\n  }\n};","map":{"version":3,"names":["SideBar","axios","test","name","data","companies","components","methods","getorders","token","window","localStorage","getItem","method","url","headers","Authorization","then","response","console","log","catch","error","created"],"sources":["E:\\Work\\Projects\\work_cycle\\src\\views\\OrdersView.vue"],"sourcesContent":["<template>\n  <div class=\"oreders\">\n    <SideBar>\n      <div class=\"container\">\n        <div class=\"table-responsive\" style=\"opacity: 0.8\">\n          <table class=\"table table-dark\">\n            <thead>\n              <tr>\n                <th>Compny name</th>\n                <th>Phone number</th>\n                <th>ID number</th>\n                <th>Commercial record</th>\n                <th>Email</th>\n                <th>Accept</th>\n                <th>Reject</th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"company in test\" :key=\"company.id\">\n                <td>{{ company.name }}</td>\n                <td>{{ company.phone }}</td>\n                <td>{{ company.ID_number }}</td>\n                <td>{{ company.commercial_number }}</td>\n                <td>{{ company.email }}</td>\n                <td>\n                  <button class=\"btn btn-primary\" :id=\"company.id\">\n                    Accept\n                  </button>\n                </td>\n                <td>\n                  <button class=\"btn btn-primary\" :id=\"company.id\">\n                    reject\n                  </button>\n                </td>\n              </tr>\n            </tbody>\n          </table>\n        </div>\n      </div>\n    </SideBar>\n  </div>\n  <!-- background -->\n  <img src=\"@/assets/Images/Background/Background3.jpg\" class=\"bgimg\" alt=\"\" />\n</template>\n\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\nimport axios from \"axios\";\nlet test = [];\nexport default {\n  name: \"OrdersView\",\n  data() {\n    return {\n      companies: test,\n    };\n  },\n  components: {\n    SideBar,\n  },\n  methods: {\n    getorders() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        method: \"get\",\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-all-orders\",\n        headers: { Authorization: `Bearer ${token}` },\n      })\n        .then(function (response) {\n          console.log(response.data);\n          test = response.data;\n        })\n        .catch(function (error) {\n          console.log(error.response);\n        });\n    },\n  },\n  created() {\n    this.getorders();\n    console.log(test);\n  },\n};\n</script>\n"],"mappings":"AA8CA,OAAOA,OAAM,MAAO,0BAA0B;AAC9C,OAAOC,KAAI,MAAO,OAAO;AACzB,IAAIC,IAAG,GAAI,EAAE;AACb,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAEH;IACb,CAAC;EACH,CAAC;EACDI,UAAU,EAAE;IACVN;EACF,CAAC;EACDO,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,MAAMC,KAAI,GAAIC,MAAM,CAACC,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;MAClDX,KAAK,CAAC;QACJY,MAAM,EAAE,KAAK;QACbC,GAAG,EAAE,iEAAiE;QACtEC,OAAO,EAAE;UAAEC,aAAa,EAAG,UAASP,KAAM;QAAE;MAC9C,CAAC,EACEQ,IAAI,CAAC,UAAUC,QAAQ,EAAE;QACxBC,OAAO,CAACC,GAAG,CAACF,QAAQ,C