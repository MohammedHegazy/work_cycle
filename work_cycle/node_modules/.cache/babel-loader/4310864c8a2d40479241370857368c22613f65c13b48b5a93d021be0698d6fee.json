{"ast":null,"code":"import SideBar from \"@/components/SideBar.vue\";\nimport $store from \"@/store/index\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      company_details: {\n        name: \"\",\n        phnum: \"\",\n        commercial_record: \"\",\n        email: \"\",\n        id: \"\",\n        IDnum: \"\"\n      },\n      Companies: []\n    };\n  },\n  components: {\n    SideBar\n  },\n  methods: {\n    loaddata() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        url: \"https://1-taqdeer.com/ServicesProject/public/api/get-company/\" + $store.state.id,\n        method: \"get\",\n        headers: {\n          Authorization: `Bearer ${token}`\n        }\n      }).then(response => {\n        console.log(response);\n        this.company_details = response.data;\n      }).catch(error => {\n        console.log(error);\n      });\n      for (let index = 0; index < this.Companies.length; index++) {\n        if (this.Companies[index].id == $store.state.id) {\n          this.company_details = this.Companies[index];\n        }\n      }\n    }\n  },\n  mounted() {\n    this.loaddata();\n  }\n};","map":{"version":3,"names":["SideBar","$store","axios","data","company_details","name","phnum","commercial_record","email","id","IDnum","Companies","components","methods","loaddata","token","window","localStorage","getItem","url","state","method","headers","Authorization","then","response","console","log","catch","error","index","length","mounted"],"sources":["E:\\Work\\Projects\\work_cycle\\src\\views\\CompanyInfo.vue"],"sourcesContent":["<template>\n  <div class=\"companyinfo\" style=\"color: black\">\n    <SideBar>\n      <div class=\"container\" style=\"max-width: 50rem\">\n        <div class=\"card bg-dark\" style=\"opacity: 0.8\">\n          <div class=\"card-body\">\n            <div class=\"card-title\" style=\"color: white\">\n              Company name: {{ company_details.name }}\n            </div>\n            <ul class=\"list-group list-group-flush\">\n              <li class=\"list-group-item\">\n                Company Phone number: {{ company_details.phnum }}\n              </li>\n              <li class=\"list-group-item\">\n                Company Commercial record:\n                {{ company_details.commercial_record }}\n              </li>\n              <li class=\"list-group-item\">\n                Company email: {{ company_details.email }}\n              </li>\n            </ul>\n          </div>\n        </div>\n      </div>\n    </SideBar>\n  </div>\n  <!-- background -->\n  <img src=\"@/assets/Images/Background/Background3.jpg\" class=\"bgimg\" alt=\"\" />\n</template>\n\n<script>\nimport SideBar from \"@/components/SideBar.vue\";\nimport $store from \"@/store/index\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      company_details: {\n        name: \"\",\n        phnum: \"\",\n        commercial_record: \"\",\n        email: \"\",\n        id: \"\",\n        IDnum: \"\",\n      },\n      Companies: [],\n    };\n  },\n  components: {\n    SideBar,\n  },\n  methods: {\n    loaddata() {\n      const token = window.localStorage.getItem(\"token\");\n      axios({\n        url:\n          \"https://1-taqdeer.com/ServicesProject/public/api/get-company/\" +\n          $store.state.id,\n        method: \"get\",\n        headers: { Authorization: `Bearer ${token}` },\n      })\n        .then((response) => {\n          console.log(response);\n          this.company_details = response.data;\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n      for (let index = 0; index < this.Companies.length; index++) {\n        if (this.Companies[index].id == $store.state.id) {\n          this.company_details = this.Companies[index];\n        }\n      }\n    },\n  },\n  mounted() {\n    this.loaddata();\n  },\n};\n</script>\n\n<style scoped>\n.card-title {\n  font-size: 30px;\n}\n\nli {\n  font-size: